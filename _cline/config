# coding: utf-8

Cline.pool_size = 2000
Cline.out_stream = Cline::OutStreams::WithGrowl.new($stdout)
Cline.collectors << Cline::Collectors::Feed
Cline.collectors << Cline::Collectors::Github

Cline::Collectors::Github.login_name = 'hibariya'

